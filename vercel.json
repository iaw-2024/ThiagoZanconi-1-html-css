{
    "version": 2,
    "routes": [
        {
            "src": "/(.*)",
            "dest": "listado/html_css/index.html"
        },
        {
            "src": "styles.css",
            "dest": "listado/html_css/styles.css"
        }
    ],
    "rewrites": [
        { "source": "styles.css", "destination": "listado/html_css/styles.css" }
      ]
  }