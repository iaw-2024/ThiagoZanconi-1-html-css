{
    "version": 2,
    "outputDirectory": "listado/html_css/index.html",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "listado/html_css/styles.css"
        },
        {
            "src": "/(.*)",
            "dest": "listado/html_css/index.html"
        }
    ],
    "builds": [
      {
        "src": "listado/html_css/index.html",
        "use": "@vercel/static"
      }
    ]
  }